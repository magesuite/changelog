<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="MageSuite_Changelog::Changelog" title="Changelog" sortOrder="10">
					<resource id="MageSuite_Changelog::Changelog_save" title="Save Changelog" sortOrder="10"/>
					<resource id="MageSuite_Changelog::Changelog_delete" title="Delete Changelog" sortOrder="20"/>
					<resource id="MageSuite_Changelog::Changelog_update" title="Update Changelog" sortOrder="30"/>
					<resource id="MageSuite_Changelog::Changelog_view" title="View Changelog" sortOrder="40"/>
					<resource id="MageSuite_Changelog::config_settings" title="Changelog settings"/>
				</resource>
				<resource id="MageSuite_Changelog::Deployment" title="Deployment" sortOrder="10">
					<resource id="MageSuite_Changelog::Deployment_save" title="Save Deployment" sortOrder="10"/>
					<resource id="MageSuite_Changelog::Deployment_delete" title="Delete Deployment" sortOrder="20"/>
					<resource id="MageSuite_Changelog::Deployment_update" title="Update Deployment" sortOrder="30"/>
					<resource id="MageSuite_Changelog::Deployment_view" title="View Deployment" sortOrder="40"/>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
